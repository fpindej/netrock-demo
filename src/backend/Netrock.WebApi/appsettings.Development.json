{
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:13008"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=13004;Database=netrock;Username=admin;Password=pwd"
  },
  "Caching": {
    "DefaultExpiration": "00:10:00",
    "Redis": {
      "Enabled": true,
      "ConnectionString": "localhost:13006",
      "Password": "redis-dev-password",
      "UseSsl": false,
      "DefaultDatabase": 0,
      "InstanceName": "netrock:",
      "ConnectTimeoutMs": 5000,
      "SyncTimeoutMs": 5000,
      "AsyncTimeoutMs": 5000,
      "AbortOnConnectFail": false,
      "ConnectRetry": 3,
      "KeepAliveSeconds": 60
    },
    "InMemory": {
      "SizeLimit": 1024,
      "ExpirationScanFrequency": "00:01:00",
      "CompactionPercentage": 0.05
    }
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "Registration": {
      "PermitLimit": 50,
      "Window": "00:00:10"
    },
    "Auth": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "Sensitive": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "AdminMutations": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    }
  },
  "Authentication": {
    "Jwt": {
      "ExpiresInMinutes": 100
    }
  },
  "Email": {
    "FrontendBaseUrl": "http://localhost:13000"
  },
  "Hosting": {
    "ForceHttps": false
  },
  "Captcha": {
    "SecretKey": "1x0000000000000000000000000000000AA"
  },
  "Cors": {
    "AllowAllOrigins": true,
    "AllowedOrigins": [],
    "PolicyName": "DefaultCorsPolicy"
  }
}
